datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

// Placeholder model required for prisma generate to succeed.
// Replace with real models as the schema evolves.
model HealthCheck {
  id        Int      @id @default(autoincrement())
  checkedAt DateTime @default(now())
}
