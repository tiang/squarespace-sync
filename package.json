{
  "name": "squarespace-sync-root",
  "version": "1.0.0",
  "description": "Squarespace-Airtable sync and dashboard monorepo",
  "private": true,
  "scripts": {
    "start": "node sync/index.js",
    "dev": "nodemon sync/index.js",
    "test": "cd sync && npx jest",
    "test:coverage": "cd sync && npx jest - -coverage",
    "sync:install": "cd sync && npm install",
    "dashboard:install": "cd dashboard && npm install",
    "dashboard:dev": "concurrently \"node dashboard/server/index.js\" \"cd dashboard && npm run dev\"",
    "dashboard:build": "cd dashboard && npm run build",
    "dashboard:start": "node dashboard/server/index.js",
    "install:all": "npm run sync:install && npm run dashboard:install",
    "iclasspro": "node iclasspro/index.js",
    "iclasspro:airtable": "node iclasspro/commands/sync-airtable.js",
    "iclasspro:full": "node iclasspro/commands/sync-full.js"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.0"
  },
  "dependencies": {
    "airtable": "^0.12.2",
    "axios": "^1.13.5",
    "dotenv": "^17.3.1",
    "winston": "^3.19.0"
  }
}
